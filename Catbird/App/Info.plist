<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>BGTaskSchedulerPermittedIdentifiers</key>
	<array>
		<string>blue.catbird.posting.retry</string>
		<string>blue.catbird.chat.refresh</string>
	</array>
	<key>DCAppAttestService</key>
	<dict>
		<key>Enabled</key>
		<true/>
	</dict>
	<key>LegalPrivacyPolicyURL</key>
	<string>https://catbird.blue/privacy</string>
	<key>LegalTermsOfServiceURL</key>
	<string>https://catbird.blue/terms</string>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsLocalNetworking</key>
		<true/>
		<key>NSExceptionDomains</key>
		<dict>
			<key>localhost</key>
			<dict>
				<key>NSTemporaryExceptionAllowsInsecureHTTPLoads</key>
				<true/>
			</dict>
		</dict>
	</dict>
	<key>NSUserActivityTypes</key>
	<array>
		<string>FeedIntentIntent</string>
	</array>
	<key>NSUserNotificationUsageDescription</key>
	<string>Catbird needs permission to send you notifications about mentions, replies, and interactions with your posts.</string>
	<key>SupportURL</key>
	<string>https://catbird.blue/support</string>
	<key>UIBackgroundModes</key>
	<array>
		<string>audio</string>
		<string>remote-notification</string>
		<string>fetch</string>
		<string>processing</string>
	</array>
	<key>UIObservationTrackingEnabled</key>
	<true/>
	<key>com.apple.developer.associated-domains</key>
	<array>
		<string>applinks:catbird.blue</string>
	</array>
</dict>
</plist>
